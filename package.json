{
  "name": "vite-plugin-network-mock",
  "version": "1.0.1",
  "description": "A Vite plugin for monitoring network requests with built-in mock capabilities",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "pnpm build:client && tsup",
    "build:client": "cd src/client && pnpm install && pnpm build",
    "dev": "tsup --watch",
    "dev:client": "cd src/client && pnpm dev"
  },
  "keywords": [
    "vite",
    "vite-plugin",
    "network",
    "mock"
  ],
  "license": "MIT",
  "dependencies": {
    "mockjs": "^1.1.0",
    "ws": "^8.14.2"
  },
  "devDependencies": {
    "@types/mockjs": "^1.0.10",
    "@types/node": "^20.10.0",
    "@types/ws": "^8.5.10",
    "tsup": "^8.0.1",
    "typescript": "^5.3.0",
    "vite": "^5.0.0"
  },
  "peerDependencies": {
    "vite": ">=3.0.0"
  }
}
